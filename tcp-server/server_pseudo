# Server pseudo code 

q = {}  # Queue data structure 
send(sock, packet,interval=5,q=q) 

    if q.get() == 2: # Send SYN + ACK 
        SEQ = rand() # Generate ISN(s)
        ACK = packet.seq + 1  # increment ISN(c)
        syn_ack_pak = packet
        syn_ack_pak.seq = SEQ
        syn_ack_pak.ack = ISN_c + 1 
        syn_ack_pak.flags = 0b000010010 # Set control flags
        sendto(syn_ack_pak.build(), (syn_ack_pak.src_host, syn_ack_pak.dst_port ) ) 
        # Send SYN+ACK 
        q.put(18)


recv(sock, client_ip ):
    data, addr = recvfrom(x)

    if tcp_control[5] == 2 : # Recv SYN
        # Convert received byte object into TCPPacket() 
        syn_pak = TCPPacket.build_pak(data)
        q.put(2)
        send(sock, syn_pak)

    if tcp_control[5] == 16 # Recv ACK
        Print('received ack')
        send(packet=TCPPacket()) 
    
