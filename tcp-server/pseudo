# Client pseudo code 

q = {}  # Queue data structure 
send(TCPPacket()) 
    if q.empty():  # Send SYN 
        - gen seq 
        sendto(syn-packet)
        q.put(2)

    elif q.get() == 18: # Received SYN+ACK 
        - SEQ = ACK 
        - ACK = SEQ + 1 
        sendto(ack-packet.build(TCPPacket().build(seq=SEQ, ack=ACK)) ) # Send ACK 
        q.put(16)

    elif q.get() == 16: # Received ACK 
        Print('received ack')

recv():
    if tcp_control[5] == 18: # Recv SYN+ACK
        q.put(18)
        # Convert received byte object into TCPPacket() 
        send(packet=TCPPacket())

    if tcp_control[5] == 16 # Recv ACK
        send(packet=TCPPacket()) 
    
